#react-root-container
  height: 100%

  .app-component
    position: relative
    height: 100%

    main
      position: relative
      height: calc(100% - 60px)
      box-sizing: border-box

      article.all-tours
        height: 100%
        overflow: auto
        -webkit-overflow-scrolling: touch
        background: #EEEEEE

        > section
            margin: 20px 0

            header
              padding: 0 20px
              margin: 0 0 10px
              color: rgba(0, 0, 0, .54)
              font-size: 16px




